<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="BasicEndpoint">
                    <security mode="TransportWithMessageCredential">
                        <transport clientCredentialType="None" />
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://webcal.local/DataService.svc" binding="wsHttpBinding"
                bindingConfiguration="BasicEndpoint" contract="WebcalDataService.IDataService"
                name="BasicEndpoint" />
        </client>
    </system.serviceModel>
</configuration>